*
    margin 0
    padding 0
    line-height 1
    font-family  Helvetica,STHeiti,Droid Sans Fallback
    box-sizing border-box
a
    text-decoration none
    color inherit
ul
ol
    list-style none
em
i
    font-style normal
.none
    display none
.color
    background rgb(34,111,187)
.footer
    height (44/16) rem
    width 100%
    background-image url(../image/footer.png)
    background-size 100% 100%
    overflow hidden
    .pic1
        margin (6/16) rem
        background-image url(../image/icon1.jpg)
        display inline-block
        width (122/16) rem
        height (33/16) rem
        background-size 100% 100%
    .pic2
        float right
        background-image url(../image/icon33.png)
        display inline-block
        width (22/16) rem
        height (20/16) rem
        background-size auto 100%
        background-position-x -(20/16) rem
        margin (14/16) rem
.fixedBox
    width 100%
    ul
        width 100%
        padding-top (5/16) rem
        height (100/16) rem
        background rgb(73,152,231)
        li
            width 20%
            float left
            text-align center
            line-height (30/16) rem
            font-size 1.1rem
            color white
.nav
    width 100%
    background rgb(62,134,206)
    ul
        width 100%
        height (40/16) rem
        padding (5/16)rem 0
        li
            width 20%
            line-height (30/16) rem
            float left
            text-align center
            color white
            font-size 1.1rem
            height (30/16) rem
.gallery
    position relative
    width 100%
    height (165/16) rem
    img
        width 100%
        height @height
    h2
        position absolute
        font-size 1.1rem
        bottom 0
        color white
        text-align center
        left 0
        right 0
        padding-right (36/16) rem
        line-height 2rem

























